{
    "name": "ui5.walkthrough",
    "version": "1.0.0",
    "author": "Lu√≠s Marques",
    "description": "UI5 TypeScript Walkthrough",
    "private": true,
    "scripts": {
        "start": "ui5 serve -o index.html",
        "start:dev": "ui5 serve -o test/mockServer.html",
        "test:unit": "ui5 serve -o test/unit/unitTests.qunit.html",
        "test:integration": "ui5 serve -o test/integration/opaTests.qunit.html",
        "build": "ui5 build --all --clean-dest",
        "serve-dist": "ws --compress -d dist --open"
    },
    "devDependencies": {
        "@types/openui5": "^1.120.0",
        "@ui5/cli": "^4.0.8",
        "@ui5/ts-interface-generator": "^0.9.0",
        "local-web-server": "^5.4.0",
        "typescript": "^5.6.2",
        "ui5-middleware-livereload": "^3.1.0",
        "ui5-middleware-serveframework": "^3.3.0",
        "ui5-middleware-simpleproxy": "^3.3.0",
        "ui5-tooling-transpile": "^3.5.0"
    }
}
